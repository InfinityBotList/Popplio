#/bin/bash
CDN_PATH=/silverpelt/cdn/ibl/dev
cd ~/ && ibl db new seed ${CDN_PATH}/seed.iblseed --db infinity --backup-tables automated_vote_resets,bot_whitelist,changelogs,partner_types,staff_template_types,staff_templates --extensions uuid-ossp\|citext\|semver,https://github.com/theory/pg-semver && ibl db gen-ci-schema /silverpelt/cdn/ibl/dev/seed-ci.json
